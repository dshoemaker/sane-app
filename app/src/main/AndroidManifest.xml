<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.umkc.dshoemaker.sane_app" >
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".RadialActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="edu.umkc.dshoemaker.sane_app.ReportActivity"
            android:label="Sign-up Page"
            android:parentActivityName=".RadialActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.umkc.dshoemaker.sane_app.ReportActivity" />
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
</manifest>
